<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.upenn.cis350.cis350_finalproject">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="SignInActivity"
            android:usesCleartextTraffic="true"/>
        <activity android:name="CreateAccountActivity"
            android:usesCleartextTraffic="true"/>
        <activity android:name="DashboardActivity"
            android:usesCleartextTraffic="true"/>
        <activity android:name="NewTimeslotActivity"
            android:usesCleartextTraffic="true"/>
        <activity android:name="RequestAppointmentActivity"
            android:usesCleartextTraffic="true"/>
        <activity android:name="AppointmentActivity"
            android:usesCleartextTraffic="true"/>
        <activity android:name="FiltersActivity"
        	android:usesCleartextTraffic="true"/>
        <activity android:name=".OtherUserProfileActivity"
            android:usesCleartextTraffic="true"/>
    </application>

</manifest>